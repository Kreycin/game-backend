    "Role": {
      "type": "string"
    },
    "Element": {
      "type": "enumeration",
      "enum": [
        "Legend",
        "Demon"
      ]
    },
    "HP": {
      "type": "integer"
    },
    "ATK": {
      "type": "integer"
    },
    "DEF": {
      "type": "integer"
    },
    "SPD": {
      "type": "integer"
    },
    "CRIT_DMG": {
      "type": "string"
    },
    "Lifesteal": {
      "type": "string"
    },
    "Penetration": {
      "type": "string"
    },
    "CRIT_rate": {
      "type": "string"
    },
    "CRIT_Res": {
      "type": "string"
    },
    "Debuff_Acc": {
      "type": "string"
    },
    "Debuff_Res": {
      "type": "string"
    },
    "Accuracy": {
      "type": "string"
    },
    "Doge": {
      "type": "string"
    },
    "Healing_Amt": {
      "type": "string"
    },
    "Healing_Amt_P": {
      "type": "string"
    },
    "Extra_DMG": {
      "type": "string"
    },
